# Application Configuration
VITE_APP_NAME=Authentication Frontend React
VITE_ENVIRONMENT=development

# API Configuration
VITE_API_BASE_URL=http://localhost:3000/api
VITE_API_KEY=your-api-key-here

# Third-party Authentication
VITE_GOOGLE_CLIENT_ID=your-google-client-id
VITE_TRUECALLER_APP_KEY=your-truecaller-app-key
VITE_FINORAMIC_CLIENT_ID=your-finoramic-client-id

# Analytics
VITE_WEBENGAGE_KEY=your-webengage-key
VITE_MOENGAGE_KEY=your-moengage-key

# Feature Flags
VITE_ENABLE_GOOGLE_AUTH=true
VITE_ENABLE_TRUECALLER_AUTH=true
VITE_ENABLE_FINORAMIC_AUTH=false
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_DEBUG_MODE=true